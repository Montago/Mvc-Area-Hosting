    </httpHandlers>

  </system.web>

  

  <system.webServer>

    <handlers>
      <!--<add name="LessHttpHandler" type="dotless.Core.LessCssHttpHandler, dotless.Core" preCondition="integratedMode" path="*.less" verb="*" />-->
